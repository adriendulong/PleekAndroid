<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@null"
    android:orientation="horizontal"
    android:gravity="center"
    android:paddingTop="10dp">

    <LinearLayout
        android:id="@+id/layoutPiki1"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:background="@drawable/bg_best_shadow"
        android:layout_weight="1">

        <RelativeLayout
            android:id="@+id/layoutPikiFrame1"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="0.75"
            android:background="@null">

            <com.pleek.app.views.RoundedImageView
                android:id="@+id/imgPiki1"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@drawable/bg_rounded_top_right_left"
                app:riv_corner_radius_top_left="1dp"
                app:riv_corner_radius_top_right="1dp"
                android:layout_alignParentTop="true"
                android:scaleType="centerCrop" />

            <ImageView
                android:id="@+id/imgPlay"
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:layout_centerInParent="true"
                android:src="@drawable/picto_play"
                android:visibility="gone" />

            <com.pleek.app.views.CircleProgressBar
                android:id="@+id/progressBarPiki1"
                app:mlpb_show_arrow="false"
                app:mlpb_enable_circle_background="false"
                app:mlpb_progress_stoke_width="2dp"
                app:mlpb_progress_text_visibility="invisible"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:layout_gravity="center"
                android:visibility="gone"
                android:layout_centerInParent="true" />
        </RelativeLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:background="@color/blancEightyTransparent" />

        <LinearLayout
            android:id="@+id/layoutReact1"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:orientation="horizontal"
            android:layout_weight="0.25"
            android:background="@null">

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@null"
                android:layout_weight="1">

                <com.pleek.app.views.RoundedImageView
                    android:id="@+id/imgReact11"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@drawable/bg_rounded_bottom_left"
                    app:riv_corner_radius_bottom_left="1dp" />

                <com.pleek.app.views.CircleProgressBar
                    android:id="@+id/progressBarReact11"
                    app:mlpb_show_arrow="false"
                    app:mlpb_enable_circle_background="false"
                    app:mlpb_progress_stoke_width="1dp"
                    app:mlpb_progress_text_visibility="invisible"
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:layout_gravity="center"
                    android:visibility="gone"
                    android:layout_centerInParent="true"/>
            </RelativeLayout>

            <View
                android:layout_width="0.5dp"
                android:layout_height="match_parent"
                android:background="@color/blancEightyTransparent" />

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@null"
                android:layout_weight="1">

                <com.pleek.app.views.BlockingImageView
                    android:id="@+id/imgReact12"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_centerInParent="true"
                    android:background="@color/fondBackReact"
                    android:scaleType="centerCrop" />

                <com.pleek.app.views.CircleProgressBar
                    android:id="@+id/progressBarReact12"
                    app:mlpb_show_arrow="false"
                    app:mlpb_enable_circle_background="false"
                    app:mlpb_progress_stoke_width="1dp"
                    app:mlpb_progress_text_visibility="invisible"
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:layout_gravity="center"
                    android:visibility="gone"
                    android:layout_centerInParent="true"/>
            </RelativeLayout>

            <View
                android:layout_width="0.5dp"
                android:layout_height="match_parent"
                android:background="@color/blancEightyTransparent" />

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@null"
                android:layout_weight="1">

                <com.pleek.app.views.RoundedImageView
                    android:id="@+id/imgReact13"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_centerInParent="true"
                    android:background="@drawable/bg_rounded_bottom_right"
                    android:scaleType="centerCrop"
                    app:riv_corner_radius_bottom_right="1dp" />

                <com.pleek.app.views.CircleProgressBar
                    android:id="@+id/progressBarReact13"
                    app:mlpb_show_arrow="false"
                    app:mlpb_enable_circle_background="false"
                    app:mlpb_progress_stoke_width="1dp"
                    app:mlpb_progress_text_visibility="invisible"
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:layout_gravity="center"
                    android:visibility="gone"
                    android:layout_centerInParent="true"/>
            </RelativeLayout>
        </LinearLayout>
    </LinearLayout>

    <View
        android:layout_width="10dp"
        android:layout_height="match_parent"
        android:background="@color/transparent" />

    <LinearLayout
        android:id="@+id/layoutPiki2"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:background="@drawable/bg_best_shadow"
