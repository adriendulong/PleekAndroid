                <com.pleek.app.views.CircleProgressBar
                    android:id="@+id/progressBarReact22"
                    app:mlpb_show_arrow="false"
                    app:mlpb_enable_circle_background="false"
                    app:mlpb_progress_stoke_width="1dp"
                    app:mlpb_progress_text_visibility="invisible"
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:layout_gravity="center"
                    android:visibility="gone"
                    android:layout_centerInParent="true"/>
            </RelativeLayout>

            <View
                android:layout_width="0.5dp"
                android:layout_height="match_parent"
                android:background="@color/blancEightyTransparent" />

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@null"
                android:layout_weight="1">

                <com.pleek.app.views.RoundedImageView
                    android:id="@+id/imgReact23"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_centerInParent="true"
                    android:background="@drawable/bg_rounded_bottom_right"
                    android:scaleType="centerCrop"
                    app:riv_corner_radius_bottom_right="1dp" />

                <com.pleek.app.views.CircleProgressBar
                    android:id="@+id/progressBarReact23"
                    app:mlpb_show_arrow="false"
                    app:mlpb_enable_circle_background="false"
                    app:mlpb_progress_stoke_width="1dp"
                    app:mlpb_progress_text_visibility="invisible"
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:layout_gravity="center"
                    android:visibility="gone"
                    android:layout_centerInParent="true"/>
            </RelativeLayout>
        </LinearLayout>
    </LinearLayout>
</LinearLayout>
