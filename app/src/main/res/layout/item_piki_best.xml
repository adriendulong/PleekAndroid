<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@null"
    android:orientation="horizontal">

    <android.support.v7.widget.CardView
        android:id="@+id/layoutPiki1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/bg_best_shadow"
        android:layout_weight="1"
        android:orientation="vertical"
        app:cardCornerRadius="4dp"
        app:cardPreventCornerOverlap="true"
        app:cardUseCompatPadding="true">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <FrameLayout
                android:id="@+id/layoutPikiFrame1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <com.pleek.app.views.BlockingImageView
                    android:id="@+id/imgPiki1"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_centerInParent="true"
                    android:background="@color/fondBackReact"
                    android:scaleType="centerCrop" />

                <com.pleek.app.views.CircleProgressBar
                    android:id="@+id/progressBarPiki1"
                    app:mlpb_show_arrow="false"
                    app:mlpb_enable_circle_background="false"
                    app:mlpb_progress_stoke_width="2dp"
                    app:mlpb_progress_text_visibility="invisible"
                    android:layout_width="60dp"
                    android:layout_height="60dp"
                    android:layout_gravity="center"
                    android:visibility="gone" />
            </FrameLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:background="@color/blanc" />

            <LinearLayout
                android:id="@+id/layoutReact1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <FrameLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1">

                    <com.pleek.app.views.BlockingImageView
                        android:id="@+id/imgReact11"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_centerInParent="true"
                        android:background="@color/fondBackReact"
                        android:scaleType="centerCrop" />

                    <com.pleek.app.views.CircleProgressBar
                        android:id="@+id/progressBarReact11"
                        app:mlpb_show_arrow="false"
                        app:mlpb_enable_circle_background="false"
                        app:mlpb_progress_stoke_width="1dp"
                        app:mlpb_progress_text_visibility="invisible"
                        android:layout_width="20dp"
                        android:layout_height="20dp"
                        android:layout_gravity="center"
                        android:visibility="gone" />
                </FrameLayout>

                <View
                    android:layout_width="0.5dp"
                    android:layout_height="match_parent"
                    android:background="@color/blanc" />

                <FrameLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1">

                    <com.pleek.app.views.BlockingImageView
                        android:id="@+id/imgReact12"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_centerInParent="true"
                        android:background="@color/fondBackReact"
                        android:scaleType="centerCrop" />

                    <com.pleek.app.views.CircleProgressBar
                        android:id="@+id/progressBarReact12"
                        app:mlpb_show_arrow="false"
                        app:mlpb_enable_circle_background="false"
                        app:mlpb_progress_stoke_width="1dp"
                        app:mlpb_progress_text_visibility="invisible"
                        android:layout_width="20dp"
                        android:layout_height="20dp"
                        android:layout_gravity="center"
                        android:visibility="gone" />
                </FrameLayout>

                <View
                    android:layout_width="0.5dp"
                    android:layout_height="match_parent"
                    android:background="@color/blanc" />

                <FrameLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1">

                    <com.pleek.app.views.BlockingImageView
                        android:id="@+id/imgReact13"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_centerInParent="true"
                        android:background="@color/fondBackReact"
                        android:scaleType="centerCrop" />

                    <com.pleek.app.views.CircleProgressBar
                        android:id="@+id/progressBarReact13"
                        app:mlpb_show_arrow="false"
                        app:mlpb_enable_circle_background="false"
                        app:mlpb_progress_stoke_width="1dp"
                        app:mlpb_progress_text_visibility="invisible"
                        android:layout_width="20dp"
                        android:layout_height="20dp"
                        android:layout_gravity="center"
                        android:visibility="gone" />
                </FrameLayout>
            </LinearLayout>
        </LinearLayout>
    </android.support.v7.widget.CardView>

